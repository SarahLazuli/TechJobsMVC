<div class="container body-content">

    <h1>@ViewBag.title</h1>

    @foreach (var job in ViewBag.jobs)
    {
        //TODO #1: Use a loop to display job results in a table.
<table class="job-listing">
    <tr><td>ID: @job.Id</td></tr>
    <tr><td>Name: @job.Name</td></tr>
    <tr><td>Employer: @job.Employer.Value</td></tr>
    <tr><td>Location: @job.Location.Value</td></tr>
    <tr><td>Position Type: @job.PositionType.Value</td></tr>
    <tr><td>Skill: @job.CoreCompetency.Value</td></tr>
</table>
    }


</div>